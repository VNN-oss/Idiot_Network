import os 
import pandas as pd
import seaborn as sns
import matplotlib.pyplot as plt
from sklearn.neural_network import MLPClassifier
from sklearn.metrics import confusion_matrix, classification_report
from sklearn.preprocessing import StandardScaler, LabelEncoder
from sklearn.model_selection import train_test_split
from pesudo_generator import generator

math_problems = Generate(runtime)
label_quality = LabelEncoder()

X = math_problems.drop("Answer", axis = 1) #
y = math_problems['Distance'] #Actual answer

X_train, X_Test, y_train,y_test = train_test_split(X,y, test_size = .2, random_state = 50)

mlpc = MLPClassifier(hidden_layer_sizes = (11,11,11), max_iter = 420)
mlpc.fit(X_train,y_train)
pred = mlpc.predict(X_Test)
print(classification_report(y_train,pred))
print(confusion_matrix(y_test,pred))
